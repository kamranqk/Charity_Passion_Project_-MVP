@model PassionProjectChariity_s2022.Models.FundDto

@{
    ViewBag.Title = "Edit";
}

<h2>Update Fund</h2>
<form action="/Fund/Update/@Model.FundID" method="POST">
    <div style="display:none;">
        @* Pass through the Fund ID *@
        <input type="text" name="FundID" value="@Model.FundID" />
    </div>
    <div>
        <label>Fund Name:</label> <input type="text" name="FundName" value="@Model.FundName" />
    </div>
    
    <div>
        <input type="submit" value="Update Fund" />

    </div>
</form>

